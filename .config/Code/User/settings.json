{
    "telemetry.telemetryLevel": "off",

    "editor.fontFamily": "'Fira Code', 'la-solid-900', 'la-brands-400', 'la-regular-400'",
    "editor.fontLigatures": true,

    "editor.fontSize": 16,
    "editor.rulers": [80, 120],

    "workbench.colorTheme": "One Dark Pro",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "welcomePage",

    "files.associations": {
        "config": "ini",
        "dunstrc": "ini",
        "*.world": "xml",
        "conda_requirements": "pip-requirements"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,

    "material-icon-theme.saturation": 0.8,

    "colorize.languages": ["ini", "plaintext", "properties"],

    "oneDarkPro.bold": true,
    "oneDarkPro.italic": false,

    "gitlens.codeLens.enabled": false,

    "[plaintext,markdown]": {
        "editor.wordWrap": "bounded",
    },
    "terminal.integrated.inheritEnv": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.latex.autoBuild.run": "never",
    "ltex.additionalRules.motherTongue": "en-AU",
    "ltex.language": "en-AU",
    "[latex]": {
        "editor.wordWrap": "wordWrapColumn"
    },
    "wordcounter.languages": [
        "latex"
    ],
    "wordcounter.side.left": [
        "word",
        "paragraph"
    ],
    "matlab.matlabpath": "/usr/local/MATLAB/R2021b/bin/matlab",
    "matlab.mlintpath": "/usr/local/MATLAB/R2021b/bin/glnxa64/mlint",
    "cmake.configureOnOpen": false,
    "python.analysis.autoImportCompletions": false
}